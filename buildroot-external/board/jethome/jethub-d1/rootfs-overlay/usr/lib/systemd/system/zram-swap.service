[Unit]
Description=HassOS ZRAM swap for JetHub D1
DefaultDependencies=no
Requires=mnt-data.mount
After=mnt-data.mount hassos-expand.service
Wants=mnt-data.mount
#RefuseManualStart=true
#RefuseManualStop=true

[Service]
Type=oneshot
ExecStart=/usr/lib/jethome/swap_file_maker >/tmp/test.log
TimeoutStartSec=5min
RemainAfterExit=true
StandardOutput=null

[Install]
WantedBy=hassos-zram.target
